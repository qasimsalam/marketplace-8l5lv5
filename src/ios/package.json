{
  "name": "@aitalent/ios",
  "version": "1.0.0",
  "private": true,
  "description": "iOS mobile application for the AI Talent Marketplace platform",
  "author": "AI Talent Marketplace Team",
  "license": "UNLICENSED",
  "main": "index.js",
  "scripts": {
    "start": "react-native start",
    "ios": "react-native run-ios",
    "ios:device": "react-native run-ios --device",
    "build:release": "react-native build-ios --configuration Release",
    "reset-cache": "react-native start --reset-cache",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "pod-install": "cd ios && pod install && cd ..",
    "postinstall": "node ./scripts/postinstall.js && yarn pod-install",
    "e2e:build": "detox build -c ios",
    "e2e:test": "detox test -c ios"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^4.0.1",
    "@expo/vector-icons": "^13.0.0",
    "@notifee/react-native": "^7.8.0",
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-native-community/datetimepicker": "^7.4.1",
    "@react-native-community/netinfo": "^9.3.10",
    "@react-native-community/push-notification-ios": "^1.11.0",
    "@react-native-firebase/app": "^18.3.0",
    "@react-native-firebase/messaging": "^18.3.0",
    "@react-native-picker/picker": "^2.5.1",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/elements": "^1.3.17",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@react-navigation/stack": "^6.3.17",
    "@reduxjs/toolkit": "^1.9.5",
    "@shopify/restyle": "^2.4.0",
    "@stripe/stripe-react-native": "^0.32.0",
    "axios": "^1.6.0",
    "date-fns": "^2.30.0",
    "eventemitter3": "^5.0.1",
    "expo": "^49.0.0",
    "expo-font": "~11.4.0",
    "expo-local-authentication": "~13.4.1",
    "expo-secure-store": "~12.3.1",
    "formik": "^2.4.2",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-hook-form": "^7.45.0",
    "react-native": "0.72.4",
    "react-native-apple-authentication": "^2.3.0",
    "react-native-biometrics": "^3.0.1",
    "react-native-code-push": "^8.0.1",
    "react-native-config": "^1.5.1",
    "react-native-device-info": "^10.6.0",
    "react-native-document-picker": "^8.1.3",
    "react-native-fast-image": "^8.6.3",
    "react-native-file-viewer": "^2.1.5",
    "react-native-flipper": "^0.200.0",
    "react-native-format-currency": "^0.0.3",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.12.0",
    "react-native-haptic-feedback": "^2.0.0",
    "react-native-image-picker": "^5.0.0",
    "react-native-keychain": "^8.1.2",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-markdown-display": "^7.0.0-alpha.2",
    "react-native-modal": "^13.0.1",
    "react-native-modal-datetime-picker": "^13.1.2",
    "react-native-permissions": "^3.8.0",
    "react-native-reanimated": "^3.3.0",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-screens": "^3.22.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "13.9.0",
    "react-native-svg-transformer": "^1.1.0",
    "react-native-syntax-highlighter": "^2.1.0",
    "react-native-tab-view": "^3.5.1",
    "react-native-toast-message": "^2.1.6",
    "react-native-vector-icons": "^9.2.0",
    "react-native-webview": "^13.6.2",
    "react-redux": "^8.1.2",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.2",
    "socket.io-client": "^4.7.2",
    "styled-components": "^6.0.7",
    "uuid": "^9.0.0",
    "validator": "^13.11.0",
    "yup": "^1.2.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.22.5",
    "@babel/runtime": "^7.22.10",
    "@jest/globals": "^29.6.2",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^12.2.2",
    "@types/jest": "^29.5.3",
    "@types/lodash": "^4.14.197",
    "@types/react": "^18.2.18",
    "@types/react-native": "^0.72.2",
    "@types/react-test-renderer": "^18.0.0",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "babel-jest": "^29.6.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "detox": "^20.11.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.2",
    "jest-mock": "^29.6.2",
    "metro-config": "^0.76.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^3.0.0",
    "react-test-renderer": "18.2.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^@components/(.*)$": "<rootDir>/src/components/$1",
      "^@screens/(.*)$": "<rootDir>/src/screens/$1",
      "^@navigation/(.*)$": "<rootDir>/src/navigation/$1",
      "^@hooks/(.*)$": "<rootDir>/src/hooks/$1",
      "^@styles/(.*)$": "<rootDir>/src/styles/$1",
      "^@utils/(.*)$": "<rootDir>/src/utils/$1",
      "^@lib/(.*)$": "<rootDir>/src/lib/$1",
      "^@store/(.*)$": "<rootDir>/src/store/$1",
      "^@types/(.*)$": "<rootDir>/src/types/$1",
      "^@assets/(.*)$": "<rootDir>/assets/$1"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|react-native-.*|@react-navigation/.*|@react-native-community/.*))"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts"
    ]
  },
  "detox": {
    "configurations": {
      "ios": {
        "type": "ios.simulator",
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/AITalentMarketplace.app",
        "build": "xcodebuild -workspace ios/AITalentMarketplace.xcworkspace -scheme AITalentMarketplace -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "device": {
          "type": "iPhone 14"
        }
      }
    },
    "test-runner": "jest"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@8.6.11"
}