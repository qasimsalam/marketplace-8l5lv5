{
  "name": "@aitalent/android",
  "version": "1.0.0",
  "private": true,
  "description": "Android mobile application for the AI Talent Marketplace platform",
  "author": "AI Talent Marketplace Team",
  "license": "UNLICENSED",
  "main": "index.js",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "android:device": "react-native run-android --device",
    "build:release": "cd android && ./gradlew assembleRelease",
    "build:bundle": "cd android && ./gradlew bundleRelease",
    "reset-cache": "react-native start --reset-cache",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "postinstall": "node ./scripts/postinstall.js",
    "e2e:build": "detox build -c android",
    "e2e:test": "detox test -c android"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@hookform/resolvers/yup": "^3.1.0",
    "@notifee/react-native": "^7.6.1",
    "@react-native-async-storage/async-storage": "^1.19.0",
    "@react-native-clipboard/clipboard": "^1.11.2",
    "@react-native-community/datetimepicker": "^7.0.1",
    "@react-native-community/netinfo": "9.3.10",
    "@react-native-community/push-notification-ios": "^1.11.0",
    "@react-native-community/slider": "4.4.2",
    "@react-native-firebase/messaging": "^18.3.0",
    "@react-native-picker/picker": "^2.4.10",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/elements": "^1.3.18",
    "@react-navigation/material-top-tabs": "^6.6.3",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@react-navigation/stack": "^6.3.17",
    "@reduxjs/toolkit": "^1.9.5",
    "@shopify/restyle": "^2.4.0",
    "axios": "^1.6.0",
    "date-fns": "^2.30.0",
    "eventemitter3": "^5.0.1",
    "expo": "^49.0.0",
    "expo-font": "~11.4.0",
    "expo-local-authentication": "~13.4.1",
    "expo-secure-store": "~12.3.1",
    "formik": "^2.4.3",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "numeral": "^2.0.6",
    "react": "18.2.0",
    "react-hook-form": "^7.43.9",
    "react-native": "0.72.4",
    "react-native-code-editor": "^1.3.3",
    "react-native-device-info": "^10.0.0",
    "react-native-document-picker": "^9.0.1",
    "react-native-fast-image": "^8.6.3",
    "react-native-file-viewer": "^2.1.5",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "2.12.1",
    "react-native-image-picker": "^5.3.1",
    "react-native-keychain": "^8.1.2",
    "react-native-markdown-display": "^7.0.0-alpha.2",
    "react-native-permissions": "^3.8.0",
    "react-native-push-notification": "^8.1.1",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.7.1",
    "react-native-screens": "^3.22.0",
    "react-native-svg": "13.9.0",
    "react-native-svg-transformer": "^1.1.0",
    "react-native-syntax-highlighter": "^2.1.0",
    "react-native-toast-notifications": "^3.4.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-webview": "^13.3.1",
    "react-redux": "^8.1.2",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.2",
    "socket.io-client": "^4.7.2",
    "uuid": "^9.0.0",
    "validator": "^13.11.0",
    "yup": "^1.1.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.22.5",
    "@babel/runtime": "^7.22.10",
    "@jest/globals": "^29.6.2",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/jest-native": "5.4.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^12.2.2",
    "@types/jest": "^29.5.3",
    "@types/react": "^18.2.18",
    "@types/react-native": "^0.72.2",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "babel-jest": "^29.6.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "detox": "^20.11.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.2",
    "jest-mock": "^29.0.0",
    "metro-config": "^0.76.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^3.0.0",
    "react-test-renderer": "18.2.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ],
    "setupFiles": [
      "./src/android/__tests__/setup.ts",
      "react-native-device-info/jest/react-native-device-info-mock",
      "@react-native-async-storage/async-storage/jest/async-storage-mock"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^@components/(.*)$": "<rootDir>/src/components/$1",
      "^@screens/(.*)$": "<rootDir>/src/screens/$1",
      "^@navigation/(.*)$": "<rootDir>/src/navigation/$1",
      "^@hooks/(.*)$": "<rootDir>/src/hooks/$1",
      "^@styles/(.*)$": "<rootDir>/src/styles/$1",
      "^@utils/(.*)$": "<rootDir>/src/utils/$1",
      "^@lib/(.*)$": "<rootDir>/src/lib/$1",
      "^@store/(.*)$": "<rootDir>/src/store/$1",
      "^@types/(.*)$": "<rootDir>/src/types/$1",
      "^@assets/(.*)$": "<rootDir>/assets/$1"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|react-native-.*|@react-navigation/.*|@react-native-community/.*))"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts"
    ]
  },
  "detox": {
    "configurations": {
      "android": {
        "type": "android.emulator",
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "cd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd ..",
        "device": {
          "avdName": "Pixel_5_API_30"
        }
      },
      "android.release": {
        "type": "android.emulator",
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "build": "cd android && ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release && cd ..",
        "device": {
          "avdName": "Pixel_5_API_30"
        }
      }
    },
    "test-runner": "jest"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@8.6.11"
}